<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:AutoUi">
    
    <Color x:Key="PrimaryColor">#0066A6</Color>
    <Color x:Key="PrimaryInverseColor">#ffffff</Color>
    <Color x:Key="SecondaryColor">#778899</Color>

    <Color x:Key="GradientColor">#778899</Color>


    <SolidColorBrush x:Key="PrimaryColorBrush" Color="{StaticResource PrimaryColor}" />
    <SolidColorBrush x:Key="PrimaryInverseColorBrush" Color="{StaticResource PrimaryInverseColor}" />
    <SolidColorBrush x:Key="SecondaryColorBrush" Color="{StaticResource SecondaryColor}" />

</ResourceDictionary>